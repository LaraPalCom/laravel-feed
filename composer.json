{
	"name": "laravelium/feed",
	"description": "Laravelium Feed package for Laravel.",
	"homepage": "https://gitlab.com/Laravelium",
	"keywords": ["laravelium", "laravel", "feed", "generator", "rss", "atom"],
	"license": "MIT",
	"authors": [
		{
			"name": "Roumen Damianoff",
			"email": "roumen@damianoff.com",
			"role": "Developer",
			"homepage": "https://damianoff.com"
		}
	],
	"support": {
		"issues": "https://gitlab.com/Laravelium/Feed/issues",
		"source": "https://gitlab.com/Laravelium/Feed",
		"wiki": "https://gitlab.com/Laravelium/Feed/wikis/home"
	},
	"require": {
		"php": ">=7.2",
		"illuminate/support": "~6.0",
		"illuminate/filesystem": "~6.0"
	},
	"require-dev": {
		"laravel/framework": "~6.0",
		"phpunit/phpunit": "^7.5 || ^8.0",
		"orchestra/testbench-core": "3.8.*"
	},
	"autoload": {
		"psr-0": {
			"Laravelium\\Feed": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Laravelium\\Feed\\FeedServiceProvider"
			],
			"aliases": {
        "Feed": "Laravelium\\Feed\\Feed"
      }
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
