{
	"name": "laravelium/feed",
	"description": "Laravelium Feed package for Laravel.",
	"homepage": "https://gitlab.com/Laravelium",
	"keywords": ["laravelium", "laravel", "feed", "generator", "rss", "atom"],
	"license": "MIT",
	"authors": [
		{
			"name": "Rumen Damyanov",
			"email": "r@alfamatter.com",
			"role": "Developer",
			"homepage": "https://darumen.com"
		}
	],
	"support": {
		"issues": "https://gitlab.com/Laravelium/Feed/issues",
		"source": "https://gitlab.com/Laravelium/Feed",
		"wiki": "https://gitlab.com/Laravelium/Feed/wikis/home"
	},
	"require": {
		"php": ">=7.2.5",
		"illuminate/support": "^7.0",
		"illuminate/filesystem": "^7.0"
	},
	"require-dev": {
		"laravel/framework": "^7.0",
		"phpunit/phpunit": "^8.0",
		"orchestra/testbench-core": "^5.0"
	},
	"autoload": {
		"psr-0": {
			"Laravelium\\Feed": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Laravelium\\Feed\\FeedServiceProvider"
			],
			"aliases": {
        "Feed": "Laravelium\\Feed\\Feed"
      }
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
